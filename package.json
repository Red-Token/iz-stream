	"version": "0.0.4",
	"main": ".vite/main/main.js",
	"type": "module",
	"scripts": {
		"start": "tsx node_modules/@electron-forge/cli/src/electron-forge-start",
		"package": "tsx node_modules/@electron-forge/cli/src/electron-forge-package",
		"make": "tsx node_modules/@electron-forge/cli/src/electron-forge-make",
		"dev": "vite dev -c ./vite.renderer.config.ts",
		"build": "vite build -c ./vite.renderer.config.ts",
		"preview": "vite preview -c ./vite.renderer.config.ts",
		"patch": "npm version patch",
		"snapshot": "npm version prerelease --preid snapshot --no-git-tag-version",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --fail-on-warnings --output human",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "prettier --check . && eslint . --no-cache",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run"
	},
	"files": [
		"build/**/*",
		"README.md",
		"LICENSE"
	],
